{
  "name": "UAM-C",
  "description": "UAM-C Allianz Experience",
  "logo_uri": "",
  "callbacks": [
    "__APP_URL__/callback"
  ],
  "allowed_origins": [
    "__APP_URL__"
  ],
  "web_origins": [
    "__APP_URL__"
  ],
  "allowed_clients": [],
  "allowed_logout_urls": [
    "__APP_URL__/logout"
  ],
  "grant_types": [
    "client_credentials",
    "password"
  ],
  "token_endpoint_auth_method": "client_secret_post",
  "app_type": "non_interactive",
  "oidc_conformant": true,
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 36000
  },
  "sso": false,
  "cross_origin_auth": false,
  "custom_login_page": "@@login.html@@",
  "custom_login_page_on": true
}
