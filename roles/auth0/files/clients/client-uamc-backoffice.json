{
  "name": "UAM-C Back-Office",
  "description": "UAM-C Back Office",
  "logo_uri": "https://www.allianz.co.uk/content/dam/allianzcouk/images/Gen2-images/AZ_Logo_RGB-2.png",
  "callbacks": [
    "__APP_URL__/callback"
  ],
  "allowed_origins": [
    "__APP_URL__"
  ],
  "web_origins": [
    "__APP_URL__"
  ],
  "allowed_clients": [],
  "allowed_logout_urls": [
    "__APP_URL__/login"
  ],
  "grant_types": [
    "implicit",
    "password"
  ],
  "token_endpoint_auth_method": "client_secret_post",
  "app_type": "regular_web",
  "oidc_conformant": true,
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 36000
  },
  "sso": false,
  "cross_origin_auth": false,
  "custom_login_page": "@@login.html@@",
  "custom_login_page_on": true
}
